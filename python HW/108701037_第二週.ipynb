    "## 我有一棵樹"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 2,
   "metadata": {},
   "outputs": [
    {
     "name": "stdout",
     "output_type": "stream",
     "text": [
      "     *\n",
      "    ***\n",
      "   *****\n",
      "  *******\n",
      " *********\n",
      "***********\n"
     ]
    }
   ],
   "source": [
    "def tree(step):\n",
    "    a=\" \"\n",
    "    b=\"*\"\n",
    "    for i in range(1,step+1):\n",
    "        s=step-i\n",
    "        t=i*2-1\n",
    "        print(a*s+\"*\"*t)\n",
    "tree(6)"
   ]
